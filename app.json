{
	"expo": {
		"name": "OpenCV",
		"slug": "OpenCV",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "opencv",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": false,
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"permissions": ["android.permission.CAMERA"],
			"package": "com.zeroonemn.OpenCV"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"react-native-vision-camera",
				{
					"cameraPermissionText": "Allow $(PRODUCT_NAME) to access your camera",
					"microphonePermissionText": "Allow $(PRODUCT_NAME) to access your microphone",
					"enableFrameProcessors": true
				}
			],
			[
				"expo-build-properties",
				{
					"android": {
						"minSdkVersion": 26,
						"compileSdkVersion": 35,
						"targetSdkVersion": 35
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "7f8c895c-4a7c-4496-ba75-a7cc33c8ee54"
			}
		},
		"owner": "zeroonemn"
	}
}
